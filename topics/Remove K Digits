class Solution:
    def removeKdigits(self, num: str, k: int) -> str:
        stack = [str(num[0])]
        for i in range(1, len(num)):
            while stack and stack[-1] > num[i] and k > 0:
                stack.pop()
                k -= 1
            
            stack.append(str(num[i]))
        for _ in range(k):
            stack.pop()
            
        s = int(''.join(stack)) if stack else '0'
        return str(s)
