class Solution:
    def searchRange(self, nums: List[int], target: int) -> List[int]:
        def first(nums,target,isfirst):
            first = -1
            left=0
            right=len(nums)-1
            while left<=right:
                mid = left + ((right-left)//2)
                if nums[mid] > target :
                    right = mid - 1
                elif nums[mid] < target :
                    left = mid + 1
                if nums[mid] == target :
                    first = mid 
                    if isfirst:
                        right = mid - 1
                    else:
                        left = mid + 1
            return first
        firstt = first(nums,target,True)
        last = first(nums,target,False)
        return [firstt,last]
        
            
