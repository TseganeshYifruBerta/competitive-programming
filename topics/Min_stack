class MinStack:

    def __init__(self):
        self.n=[]
        

    def push(self, val: int) -> None:
        return self.n.append(val)
        

    def pop(self) -> None:
        return self.n.pop()
        

    def top(self) -> int:
        return self.n[-1]
        

    def getMin(self) -> int:
        return min(self.n)
        


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(val)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
